# .env.example
# 复制此文件为 .env 并填入实际配置

# Redis
REDIS_URL=redis://localhost:6379

# 数据库（选择一个）
DATABASE_URL=postgres://embed:embed_pass@localhost:5432/embed_service?sslmode=disable
# DATABASE_URL=file:embed.db?cache=shared&mode=rwc

# GDStudio API
GD_API_BASE=https://music-api.gdstudio.xyz

# Navidrome
NAVIDROME_BASE_URL=http://localhost:4533
NAVIDROME_USER=admin
NAVIDROME_PASSWORD=admin
# 或使用 Token/Salt 认证
# NAVIDROME_TOKEN=your-token-here
# NAVIDROME_SALT=your-salt-here

# Worker 配置
MAX_CONCURRENT_JOBS=3
DOWNLOAD_TIMEOUT=600
LOG_LEVEL=info

# 安全配置
API_KEY=your-secure-api-key-here
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=60

# 存储路径（Docker 部署时使用容器内路径）
WORK_DIR=/work/tmp
MUSIC_DIR=/music/library
